@{
    ViewBag.Title = "Home Page";
}

<body background="https://previews.123rf.com/images/malven/malven1406/malven140600057/28899654-textura-de-una-antigua-alba%C3%B1iler%C3%ADa-en-tonos-marrones-claros-para-los-fondos-abstractos-y-de-fondo-de-pant.jpg"></body>

<div class="jumbotron" style="background-color:cadetblue">
    <h1>Pedidos J&B</h1>
    
</div>

</div>
<div id="myCarousel" class="carousel slide" data-ride="carousel" style="background-color:aquamarine">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
        <div class="item active center-block" >
            <img src="http://www.dsa.la/sites/default/files/styles/imagen_blog_dsa/public/img-boletin/ima_173.jpg?itok=X1kT0H6Z" alt="New York" width="600px" height="900px">
        </div>

        <div class="item center-block" >
            <img src="http://www.pqs.pe/sites/default/files/styles/852x479/public/archivos/2015/actualidad/09/cguzman/pqs-relaciones-clientes.jpg?itok=cz0wwqSc" alt="New York" width="600px" height="900px" >
        </div>


        <div class="item center-block" >
            <img src="https://media.enfasis.com/adjuntos/147/imagenes/000/091/0000091193.jpg" alt="New York" width="600px" height="900px">
        </div>

    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
    </a>
</div>